  "main": "dist/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build:docs": "react-scripts build",
    "build": "SET NODE_ENV=production && rm -rf dist && mkdir dist && npx babel src/react-advanced-news-ticker --out-dir dist --copy-files",
